  </div>

  <div class="field">
    <%= f.label :streetAddress %>
    <%= f.text_field :streetAddress %>
  </div>

  <div class="field">
    <%= f.label :suburb %>
    <%= f.text_field :suburb %>
  </div>

  <div class="field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class="field">
    <%= f.label :zip %>
    <%= f.number_field :zip %>
  </div>

  <div class="field">
    <%= f.label :photo %>
    <%= f.text_field :photo %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
